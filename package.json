{
  "name": "mybrandserver",
  "version": "1.0.0",
  "description": "This is  my Brand and Portfolio webApplicatin back end",
  "main": "Server/index.js",
  "scripts": {
    "build": "babel Server -s -d dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node Server/index.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles ",
    "test:watch": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --watch",
    "coverage": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --coverage",
    "coveralls": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Nishimwe Elysee",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@shelf/jest-mongodb": "^1.2.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint-config-esnext": "^4.1.0",
    "express": "^4.17.1",
    "heroku": "^7.43.0",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "mongo": "^0.1.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.5",
    "multer": "^1.4.2",
    "sharp": "^0.26.0",
    "sinon": "^9.1.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/Server/config/jest.setup.js"
    ],
    "testMatch": [
      "<rootDir>/Server/__tests__/**/*.test.js"
    ]
  }
}
